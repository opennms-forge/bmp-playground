<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
     http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
    <key id="breadcrumb-strategy" for="graphml" attr.name="breadcrumb-strategy" attr.type="string"/>
    <key id="label" for="all" attr.name="label" attr.type="string"></key>
    <key id="description" for="graph" attr.name="description" attr.type="string"></key>
    <key id="namespace" for="graph" attr.name="namespace" attr.type="string"></key>
    <key id="focus-strategy" for="graph" attr.name="focus-strategy" attr.type="string"></key>
    <key id="focus-ids" for="graph" attr.name="focus-ids" attr.type="string"></key>
    <key id="foreignSource" for="node" attr.name="foreignSource" attr.type="string"></key>
    <key id="foreignID" for="node" attr.name="foreignID" attr.type="string"></key>
    <key id="preferred-layout" for="graph" attr.name="preferred-layout" attr.type="string"></key>
    <key id="vertex-status-provider" for="graph" attr.name="vertex-status-provider" attr.type="boolean"></key>
    <key id="iconKey" for="node" attr.name="iconKey" attr.type="string"></key>
    <key id="semantic-zoom-level" for="graph" attr.name="semantic-zoom-level" attr.type="int"/>
    <data key="label">Network Layer 3</data>
    <data key="breadcrumb-strategy">NONE</data>

    <graph id="layer3-topology" edgedefault="undirected">
        <data key="namespace">layer3-network</data>
        <data key="focus-strategy">ALL</data>
        <data key="vertex-status-provider">true</data>

        <!-- Local System -->
        <node id="cortex">
            <data key="foreignSource">opennms-stack</data>
            <data key="label">cortex</data>
            <data key="foreignID">cortex</data>
            <data key="iconKey">server</data>
        </node>
        <node id="database">
            <data key="foreignSource">opennms-stack</data>
            <data key="label">database</data>
            <data key="foreignID">database</data>
            <data key="iconKey">database</data>
        </node>
        <node id="sentinel-dc-12">
            <data key="foreignSource">opennms-stack</data>
            <data key="label">cortex</data>
            <data key="foreignID">cortex</data>
            <data key="iconKey">server</data>
        </node>
        <node id="horizon">
            <data key="foreignSource">opennms-stack</data>
            <data key="label">horizon</data>
            <data key="foreignID">horizon</data>
            <data key="iconKey">opennms_server</data>
        </node>
        <node id="grafana">
            <data key="foreignSource">opennms-stack</data>
            <data key="label">grafana</data>
            <data key="foreignID">grafana</data>
            <data key="iconKey">server</data>
        </node>

        <!-- BGP Router Data Center -->
        <node id="rt-dc-37">
            <data key="foreignSource">bgp-routers</data>
            <data key="label">rt-dc-37</data>
            <data key="foreignID">rt-dc-37</data>
            <data key="iconKey">server</data>
        </node>
        <node id="rt-dc-38">
            <data key="foreignSource">bgp-routers</data>
            <data key="label">rt-dc-38</data>
            <data key="foreignID">rt-dc-38</data>
            <data key="iconKey">server</data>
        </node>
        <node id="rt-dc-39">
            <data key="foreignSource">bgp-routers</data>
            <data key="label">rt-dc-39</data>
            <data key="foreignID">rt-dc-39</data>
            <data key="iconKey">server</data>
        </node>

        <!-- BGP Router location 01 -->
        <node id="rt-lo01-69">
            <data key="foreignSource">bgp-routers</data>
            <data key="label">rt-lo01-69</data>
            <data key="foreignID">rt-lo01-69</data>
            <data key="iconKey">server</data>
        </node>
        <node id="rt-lo01-70">
            <data key="foreignSource">bgp-routers</data>
            <data key="label">rt-lo01-70</data>
            <data key="foreignID">rt-lo01-70</data>
            <data key="iconKey">server</data>
        </node>
        <node id="rt-lo01-71">
            <data key="foreignSource">bgp-routers</data>
            <data key="label">rt-lo01-71</data>
            <data key="foreignID">rt-lo01-71</data>
            <data key="iconKey">server</data>
        </node>

        <!-- Networks -->
        <node id="net-onms">
            <data key="label">192.168.42.0/27</data>
            <data key="iconKey">vmware-network</data>
        </node>
        <node id="net-mgmt-dc">
            <data key="label">192.168.42.32/27</data>
            <data key="iconKey">vmware-network</data>
        </node>
        <node id="net-mgmt-lo01">
            <data key="label">192.168.42.64/27</data>
            <data key="iconKey">vmware-network</data>
        </node>
        <node id="ptp-100-101">
            <data key="label">192.168.42.96/29</data>
            <data key="iconKey">vmware-network</data>
        </node>
        <node id="ptp-108-109">
            <data key="label">192.168.42.104/29</data>
            <data key="iconKey">vmware-network</data>
        </node>
        <node id="ptp-116-117">
            <data key="label">192.168.42.112/29</data>
            <data key="iconKey">vmware-network</data>
        </node>
        <node id="ptp-124-125">
            <data key="label">192.168.42.120/29</data>
            <data key="iconKey">vmware-network</data>
        </node>

        <!-- Data Center -->
        <node id="data-center">
            <data key="foreignSource">Minions</data>
            <data key="label">minion-dc-20</data>
            <data key="foreignID">minion-dc-20</data>
        </node>

        <!-- Location 01 -->
        <node id="location-01">
            <data key="foreignSource">Minions</data>
            <data key="label">minion-lo01-24</data>
            <data key="foreignID">minion-lo01-24</data>
        </node>

        <edge id="cortex-net-onms" source="cortex" target="net-onms"/>
        <edge id="database-net-onms" source="database" target="net-onms"/>
        <edge id="sentinel-dc-12-net-onms" source="sentinel-dc-12" target="net-onms"/>
        <edge id="horizon-net-onms" source="horizon" target="net-onms"/>
        <edge id="grafana-net-onms" source="grafana" target="net-onms"/>

        <edge id="minion-dc-20-net-onms" source="minion-dc-20" target="net-onms"/>
        <edge id="minion-dc-20-net-mgmt-lo01" source="minion-01" target="net-mgmt-lo01"/>

        <edge id="rt-dc-37-net-mgmt-dc" source="rt-dc-37" target="net-mgmt-dc"/>
        <edge id="rt-dc-38-net-mgmt-dc" source="rt-dc-38" target="net-mgmt-dc"/>
        <edge id="rt-dc-39-net-mgmt-dc" source="rt-dc-39" target="net-mgmt-dc"/>

        <edge id="minion-lo01-24-net-mgmt-lo01" source="minion-lo01-24" target="net-onms"/>
        <edge id="minion-lo01-24-net-mgmt-lo01" source="minion-lo01-24" target="net-mgmt-lo01"/>

        <edge id="rt-lo01-69-net-mgmt-lo01" source="rt-lo01-69" target="net-mgmt-lo01"/>
        <edge id="rt-lo01-70-net-mgmt-lo01" source="rt-lo01-70" target="net-mgmt-lo01"/>
        <edge id="rt-lo01-71-net-mgmt-lo01" source="rt-lo01-71" target="net-mgmt-lo01"/>
    </graph>
</graphml>
